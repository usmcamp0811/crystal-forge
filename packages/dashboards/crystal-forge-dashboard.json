          "rawSql": "SELECT\n  COALESCE(SUM(CASE WHEN h.heartbeat_status = 'Healthy' THEN 1 ELSE 0 END), 0) AS \"Healthy\",\n  COALESCE(SUM(CASE WHEN h.heartbeat_status = 'Warning' THEN 1 ELSE 0 END), 0) AS \"Warning\",\n  COALESCE(SUM(CASE WHEN h.heartbeat_status = 'Critical' THEN 1 ELSE 0 END), 0) AS \"Critical\",\n  COALESCE(SUM(CASE WHEN h.heartbeat_status = 'Offline' THEN 1 ELSE 0 END), 0) AS \"Offline\"\nFROM view_system_heartbeat_status AS h\nWHERE h.hostname IN (\n  SELECT s.hostname\n  FROM systems AS s\n  WHERE s.flake_id IN (${flake:csv})\n);\n",
          "refId": "A",
